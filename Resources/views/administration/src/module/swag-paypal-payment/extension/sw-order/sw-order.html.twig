{% block sw_order_detail_base %}
    <div>
        <template v-if="currentOrder.transactions[0]">
            <template v-if="currentOrder.transactions[0].paymentMethod.meta.viewData.technicalName === 'SwagPayPal'">
                {% block swag_paypal_order_extension_tabs %}
                    <sw-tabs>
                        <sw-tabs-item :route="{ name: 'sw.order.detail', params: { id: $route.params.id } }"
                                      :title="$tc('swag-paypal.tabs.overview')">
                            {{ $tc('swag-paypal.tabs.overview') }}
                        </sw-tabs-item>

                        <sw-tabs-item :route="{ name: 'swag.paypal.payment.detail', params: { id: $route.params.id } }"
                                      :title="$tc('swag-paypal.header')">
                            {{ $tc('swag-paypal.header') }}
                        </sw-tabs-item>
                    </sw-tabs>
                {% endblock %}
            </template>
        </template>
        {% parent %}
    </div>
{% endblock %}