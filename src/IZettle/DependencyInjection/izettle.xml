<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Swag\PayPal\IZettle\DataAbstractionLayer\Entity\IZettleSalesChannelDefinition">
            <tag name="shopware.entity.definition" entity="swag_paypal_izettle_sales_channel"/>
        </service>

        <service id="Swag\PayPal\IZettle\DataAbstractionLayer\Extension\SalesChannelExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="Swag\PayPal\IZettle\DataAbstractionLayer\Extension\ProductExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="Swag\PayPal\IZettle\IZettleSyncController" public="true">
            <argument type="service" id="sales_channel.repository"/>
            <argument type="service" id="Swag\PayPal\IZettle\Run\Task\CompleteTask"/>
            <argument type="service" id="Swag\PayPal\IZettle\Run\Task\ProductTask"/>
            <argument type="service" id="Swag\PayPal\IZettle\Run\Task\ImageTask"/>
            <argument type="service" id="Swag\PayPal\IZettle\Run\Task\InventoryTask"/>
            <argument type="service" id="Swag\PayPal\IZettle\Run\Administration\LogCleaner"/>
            <argument type="service" id="Swag\PayPal\IZettle\Sync\ProductSelection"/>
        </service>

        <service id="Swag\PayPal\IZettle\Sync\ProductSelection">
            <argument type="service" id="sales_channel.product.repository"/>
            <argument type="service" id="Shopware\Core\Content\ProductStream\Service\ProductStreamBuilder"/>
            <argument type="service" id="Shopware\Core\System\SalesChannel\Context\SalesChannelContextFactory"/>
        </service>

        <service id="Swag\PayPal\IZettle\Api\Service\Converter\UuidConverter"/>
        <service id="Swag\PayPal\IZettle\Api\Service\ApiKeyDecoder"/>

        <service id="Swag\PayPal\IZettle\Client\TokenClientFactory">
            <argument type="service" id="Swag\PayPal\Util\Logger"/>
            <argument type="service" id="Swag\PayPal\IZettle\Api\Service\ApiKeyDecoder"/>
        </service>

        <service id="Swag\PayPal\IZettle\Client\IZettleClientFactory">
            <argument type="service" id="Swag\PayPal\IZettle\Resource\TokenResource"/>
            <argument type="service" id="Swag\PayPal\Util\Logger"/>
        </service>

        <service id="Swag\PayPal\IZettle\Resource\TokenResource">
            <argument type="service" id="cache.object"/>
            <argument type="service" id="Swag\PayPal\IZettle\Client\TokenClientFactory"/>
        </service>

        <service id="Swag\PayPal\IZettle\Resource\UserResource">
            <argument type="service" id="Swag\PayPal\IZettle\Client\IZettleClientFactory"/>
        </service>

        <service id="Swag\PayPal\IZettle\Setting\SettingsController" public="true">
            <argument type="service" id="Swag\PayPal\IZettle\Setting\Service\ApiCredentialService"/>
            <argument type="service" id="Swag\PayPal\IZettle\Setting\Service\InformationFetchService"/>
            <argument type="service" id="Swag\PayPal\IZettle\Setting\Service\InformationDefaultService"/>
            <argument type="service" id="Swag\PayPal\IZettle\Setting\Service\ProductVisibilityCloneService"/>
        </service>

        <service id="Swag\PayPal\IZettle\Setting\Service\ApiCredentialService">
            <argument type="service" id="Swag\PayPal\IZettle\Resource\TokenResource"/>
        </service>

        <service id="Swag\PayPal\IZettle\Setting\Service\InformationFetchService">
            <argument type="service" id="Swag\PayPal\IZettle\Resource\UserResource"/>
            <argument type="service" id="country.repository"/>
            <argument type="service" id="currency.repository"/>
            <argument type="service" id="language.repository"/>
        </service>

        <service id="Swag\PayPal\IZettle\Setting\Service\InformationDefaultService">
            <argument type="service" id="customer_group.repository"/>
            <argument type="service" id="category.repository"/>
            <argument type="service" id="Shopware\Core\Framework\Plugin\Util\PluginIdProvider"/>
            <argument type="service" id="payment_method.repository"/>
            <argument type="service" id="rule.repository"/>
            <argument type="service" id="delivery_time.repository"/>
            <argument type="service" id="shipping_method.repository"/>
        </service>

        <service id="Swag\PayPal\IZettle\Setting\Service\ProductVisibilityCloneService">
            <argument type="service" id="product_visibility.repository"/>
        </service>

        <service id="Swag\PayPal\IZettle\Command\IZettleSyncCommand">
            <argument type="service" id="sales_channel.repository"/>
            <argument type="service" id="Swag\PayPal\IZettle\Run\Task\CompleteTask"/>
            <tag name="console.command"/>
        </service>

        <service id="Swag\PayPal\IZettle\Run\Task\AbstractTask" abstract="true">
            <argument type="service" id="Swag\PayPal\IZettle\Run\RunService"/>
            <argument type="service" id="Swag\PayPal\IZettle\Run\Logger"/>
        </service>

        <service id="Swag\PayPal\IZettle\Run\Task\CompleteTask" parent="Swag\PayPal\IZettle\Run\Task\AbstractTask">
            <argument type="service" id="Swag\PayPal\IZettle\Sync\ProductSyncer"/>
            <argument type="service" id="Swag\PayPal\IZettle\Sync\ImageSyncer"/>
            <argument type="service" id="Swag\PayPal\IZettle\Sync\InventorySyncer"/>
        </service>

        <service id="Swag\PayPal\IZettle\Run\Task\ProductTask" parent="Swag\PayPal\IZettle\Run\Task\AbstractTask">
            <argument type="service" id="Swag\PayPal\IZettle\Sync\ProductSyncer"/>
        </service>

        <service id="Swag\PayPal\IZettle\Run\Task\ImageTask" parent="Swag\PayPal\IZettle\Run\Task\AbstractTask">
            <argument type="service" id="Swag\PayPal\IZettle\Sync\ImageSyncer"/>
        </service>

        <service id="Swag\PayPal\IZettle\Run\Task\InventoryTask" parent="Swag\PayPal\IZettle\Run\Task\AbstractTask">
            <argument type="service" id="Swag\PayPal\IZettle\Sync\InventorySyncer"/>
        </service>
    </services>
</container>
