<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Swag\PayPal\OrdersApi\Administration\PayPalOrdersController" public="true">
            <argument type="service" id="Swag\PayPal\PayPal\ApiV2\Resource\OrderResource"/>
            <argument type="service" id="Swag\PayPal\PayPal\ApiV2\Resource\AuthorizationResource"/>
            <argument type="service" id="Swag\PayPal\PayPal\ApiV2\Resource\CaptureResource"/>
            <argument type="service" id="Swag\PayPal\PayPal\ApiV2\Resource\RefundResource"/>
            <argument type="service" id="order_transaction.repository"/>
            <argument type="service" id="Swag\PayPal\Util\PaymentStatusUtilV2"/>
            <argument type="service" id="Swag\PayPal\Util\PriceFormatter"/>
        </service>

        <service id="Swag\PayPal\OrdersApi\Builder\CartOrderBuilder">
            <argument type="service" id="Swag\PayPal\Setting\Service\SettingsService"/>
            <argument type="service" id="Swag\PayPal\Util\PriceFormatter"/>
        </service>

        <service id="Swag\PayPal\OrdersApi\Builder\OrderOrderBuilder">
            <argument type="service" id="Swag\PayPal\Setting\Service\SettingsService"/>
            <argument type="service" id="Swag\PayPal\Util\PriceFormatter"/>
        </service>
    </services>
</container>
