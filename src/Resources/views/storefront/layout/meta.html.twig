{% sw_extends '@Storefront/layout/meta.html.twig' %}

{% block layout_head_stylesheet %}
    {{ parent() }}
    {% set isHMRMode = false %}

    {% if isHMRMode %}
        <link rel="stylesheet"
                href="http://localhost:9000/css/swag-paypal-storefront.bundle.css">
    {% else %}
        <link rel="stylesheet"
                href="{{ asset('bundles/swagpaypal/static/storefront/css/swag-paypal-storefront.bundle.css') }}">
    {% endif %}
{% endblock %}