{% block swag_paypal_izettle_detail_logs %}
    {% block swag_paypal_izettle_detail_logs_grid %}
        <sw-data-grid :columns="columns"
                      :dataSource="logs"
                      :isLoading="isLoading || loadingLogs"
                      :showActions="false"
                      :showSelection="false">

            {% block swag_paypal_izettle_detail_logs_grid_pagination %}
                <template #pagination>
                    <sw-pagination :limit="logLimit"
                                   :page="logPage"
                                   :total="logTotal"
                                   :total-visible="7"
                                   @page-change="onPaginateLogs">
                    </sw-pagination>
                </template>
            {% endblock %}

            {% block swag_paypal_izettle_detail_logs_grid_column_date %}
                <template #column-date="{ item }">

                    {% block swag_paypal_izettle_detail_logs_grid_column_date_formatted %}
                        <template v-if="item.createdAt">
                            {{ item.createdAt | date({
                                hour: '2-digit',
                                minute: '2-digit',
                                day: '2-digit',
                                month: '2-digit',
                                year: '2-digit'
                            })
                            }}
                        </template>
                    {% endblock %}

                    {% block swag_paypal_izettle_detail_logs_grid_column_date_empty %}
                        <template v-else>
                            {{ $tc('swag-paypal-izettle.detail.logs.noEntry') }}
                        </template>
                    {% endblock %}

                </template>
            {% endblock %}

            {% block swag_paypal_izettle_detail_logs_grid_column_log %}
                <template #column-log="{ item }">
                    {{ item.message }}
                </template>
            {% endblock %}

            {% block swag_paypal_izettle_detail_logs_grid_column_state %}
                <template #column-state="{ item }">

                    {% block swag_paypal_izettle_detail_logs_grid_column_state_label %}
                        <sw-label v-if="item.level"
                                  appearance="pill"
                                  :variant="getLabelVariantForItem(item)">
                            {{ $tc(getLabelForItem(item)) }}
                        </sw-label>
                    {% endblock %}

                </template>
            {% endblock %}

        </sw-data-grid>
    {% endblock %}
{% endblock %}
