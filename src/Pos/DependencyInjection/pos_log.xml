<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Swag\PayPal\Pos\DataAbstractionLayer\Entity\PosSalesChannelRunDefinition">
            <tag name="shopware.entity.definition" entity="swag_paypal_pos_sales_channel_run"/>
        </service>

        <service id="Swag\PayPal\Pos\DataAbstractionLayer\Entity\PosSalesChannelRunLogDefinition">
            <tag name="shopware.entity.definition" entity="swag_paypal_pos_sales_channel_run_log"/>
        </service>

        <service id="Swag\PayPal\Pos\Run\LoggerFactory"/>

        <service id="Swag\PayPal\Pos\Run\Logger" class="Monolog\Logger">
            <factory service="Swag\PayPal\Pos\Run\LoggerFactory" method="createLogger"/>
        </service>

        <service id="Swag\PayPal\Pos\Run\RunService">
            <argument type="service" id="swag_paypal_pos_sales_channel_run.repository"/>
            <argument type="service" id="swag_paypal_pos_sales_channel_run_log.repository"/>
            <argument type="service" id="Swag\PayPal\Pos\Run\Logger"/>
        </service>

        <service id="Swag\PayPal\Pos\Run\Administration\LogCleaner">
            <argument type="service" id="swag_paypal_pos_sales_channel_run.repository"/>
        </service>

        <service id="Swag\PayPal\Pos\Command\PosLogCleanupCommand">
            <argument type="service" id="sales_channel.repository"/>
            <argument type="service" id="Swag\PayPal\Pos\Run\Administration\LogCleaner"/>
            <tag name="console.command"/>
        </service>
    </services>
</container>
