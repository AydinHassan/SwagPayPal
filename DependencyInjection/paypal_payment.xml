<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="SwagPayPal\Core\Checkout\Payment\Cart\PaymentHandler\PayPalPayment"
                 class="SwagPayPal\Core\Checkout\Payment\Cart\PaymentHandler\PayPalPayment">
            <argument type="service" id="order_transaction.repository"/>
            <argument type="service" id="SwagPayPal\PayPal\Resource\PaymentResource"/>
            <argument type="service" id="SwagPayPal\PayPal\Payment\PaymentBuilderService"/>
            <tag name="payment.method"/>
        </service>

        <service id="SwagPayPal\Controller\PayPalPaymentController"
                 class="SwagPayPal\Controller\PayPalPaymentController"
                 public="true">
            <argument type="service" id="SwagPayPal\PayPal\Resource\PaymentResource"/>
        </service>

        <service id="SwagPayPal\PayPal\Payment\PaymentBuilderService"
                 class="SwagPayPal\PayPal\Payment\PaymentBuilderService">
            <argument type="service" id="language.repository"/>
            <argument type="service" id="sales_channel.repository"/>
            <argument type="service" id="order.repository"/>
            <argument type="service" id="SwagPayPal\Setting\Service\SettingsProvider"/>
        </service>
    </services>
</container>
